    $(document).ready(function () {
        var keys = [];
        var count = [];
        var revenue = [];
        var mean = [];
        $.ajax({
            dataType: "json",
            url: "/Admin/DeviceBrowser",
            //data: data,
            success: function (data) {
                console.log(Object.keys(data).length);
                for (var i = 0; i < 5; i++) {
                    keys[i] = Object.keys(data)[i]
                    count[i] = data[Object.keys(data)[i]];
                }
                for (var i = 5; i < 10; i++) {


                    revenue[i - 5] = data[Object.keys(data)[i]];
                }
                for (var i = 10; i < 15; i++) {


                    mean[i - 10] = data[Object.keys(data)[i]];
                }


                var trace1 = {
                    type: 'bar',
                    x: count,
                    y: keys,
                    orientation: 'h',
                    name: 'Counts',
                    marker: {
                        color: 'rgb(0,124,255)'
                    }
                }
                var trace2 = {
                    type: 'bar',
                    x: revenue,
                    y: keys,
                    xaxis: 'x2',
                    yaxis: 'y2',
                    name: 'Revenue Generators',
                    orientation: 'h',
                    marker: {
                        color: 'rgb(142,124,195)'
                    }
                }
                var trace3 = {
                    type: 'bar',
                    x: mean,
                    y: keys,
                    xaxis: 'x3',
                    yaxis: 'y3',
                    name: 'Mean',
                    orientation: 'h',
                    marker: {
                        color: 'rgb(0,254,0)'
                    }
                }
                var layout = {
                    grid: { rows: 1, columns: 3, pattern: 'independent' },
                };

                var data = [trace1, trace2, trace3];


                Plotly.newPlot('browser', data, layout);
            }
        });



    })
