<!DOCTYPE html>

<html>
<head>
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <title>Trendlytics</title>
    <meta content='width=device-width, initial-scale=1.0, shrink-to-fit=no' name='viewport' />
    <link rel="icon" href="@Url.Content("~/Content/logo.ico")" type="image/x-icon" />

    <!-- Fonts and icons -->
    <script src="@Url.Content("~/Content/DashBoard/assets/js/plugin/webfont/webfont.min.js")"></script>
    <script>
        WebFont.load({
            google: { "families": ["Open+Sans:300,400,600,700"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands"], urls: ['assets/css/fonts.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });</script>

    <!-- CSS Files -->
    <link rel="stylesheet" href="@Url.Content("~/Content/DashBoard/assets/css/bootstrap.min.css")">
    <link rel="stylesheet" href="~/Content/DashBoard/assets/css/bootstrap.min.css">

    <link rel="stylesheet" href="~/Content/DashBoard/assets/css/azzara.min.css">

    <script src="https://cdn.jsdelivr.net/npm/sweetalert2@9.5.4/dist/sweetalert2.all.min.js"><</script>

    <script src="https://cdn.jsdelivr.net/npm/promise-polyfill"></script>
</head>
<body>
    <div>
        @RenderBody()
    </div>


    <script src="@Url.Content("~/Content/DashBoard/assets/js/core/jquery.3.2.1.min.js")"></script>
    <script src="@Url.Content("~/Content/DashBoard/assets/js/plugin/jquery-ui-1.12.1.custom/jquery-ui.min.js")"></script>
    <script src="@Url.Content("~/Content/DashBoard/assets/js/core/popper.min.js")"></script>
    <script src="@Url.Content("~/Content/DashBoard/assets/js/core/bootstrap.min.js")"></script>
    <script src="@Url.Content("~/Content/DashBoard/assets/js/ready.js")"></script>


    @RenderSection("scripts", required: false)
</body>
</html>
