    <script>
        WebFont.load({
            google: { "families": ["Open+Sans:300,400,600,700"] },
            custom: { "families": ["Flaticon", "Font Awesome 5 Solid", "Font Awesome 5 Regular", "Font Awesome 5 Brands"], urls: ['/Content/Dashboard/assets/css/fonts.css'] },
            active: function () {
                sessionStorage.fonts = true;
            }
        });</script>
    <script src="https://cdn.plot.ly/plotly-latest.min.js"></script>
    <link href="~/Content/DashBoard/assets/css/fonts.css" rel="stylesheet" />
    <!-- CSS Files -->
