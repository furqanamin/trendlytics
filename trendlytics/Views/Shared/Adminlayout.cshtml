                                <i class="fa fa-search"></i>
                            </a>
                        </li>
                        <li class="nav-item dropdown hidden-caret">
                            <a href="#" class="nav-link" onclick=" location.href='@Url.Action("SignOut","Admin")'">
                                <i class="fas fa-sign-out-alt"></i>
                            </a>

                        </li>

                    </ul>
                </div>
            </nav>
            <!-- End Navbar -->
        </div>

